{
  "name": "@fidoo-blog/service",
  "version": "1.0.0",
  "description": "Fidoo Blog 后端服务",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start": "nest start",
    "dev": "PORT=3005 nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d src/database/data-source.ts",
    "migration:run": "ts-node -r tsconfig-paths/register src/database/run-migration.ts",
    "migration:revert": "ts-node -r tsconfig-paths/register src/database/revert-migration.ts",
    "seed": "ts-node -r tsconfig-paths/register src/database/seeds/run-seed.ts",
    "seed:large": "ts-node -r tsconfig-paths/register src/database/seeds/generate-large-data.ts",
    "seed:permission": "ts-node -r tsconfig-paths/register src/database/seeds/init-permission-system.ts",
    "seed:dictionaries": "ts-node -r tsconfig-paths/register src/database/seeds/init-dictionaries.ts",
    "seed:fix-admin": "ts-node -r tsconfig-paths/register src/database/seeds/fix-admin-role.ts",
    "seed:audit-logs": "ts-node -r tsconfig-paths/register src/database/seeds/add-audit-logs-menu.ts",
    "seed:fix-audit-icon": "ts-node -r tsconfig-paths/register src/database/seeds/fix-audit-logs-icon.ts",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@fidoo-blog/shared": "workspace:*",
    "@nestjs/bull": "catalog:",
    "@nestjs/cache-manager": "catalog:",
    "@nestjs/common": "catalog:",
    "@nestjs/config": "catalog:",
    "@nestjs/core": "catalog:",
    "@nestjs/event-emitter": "catalog:",
    "@nestjs/jwt": "catalog:",
    "@nestjs/passport": "catalog:",
    "@nestjs/platform-express": "catalog:",
    "@nestjs/platform-socket.io": "catalog:",
    "@nestjs/schedule": "catalog:",
    "@nestjs/swagger": "catalog:",
    "@nestjs/throttler": "catalog:",
    "@nestjs/typeorm": "catalog:",
    "@nestjs/websockets": "catalog:",
    "axios": "catalog:",
    "bcrypt": "catalog:",
    "bull": "catalog:",
    "cache-manager": "catalog:",
    "cache-manager-redis-yet": "catalog:",
    "class-transformer": "catalog:",
    "class-validator": "catalog:",
    "dayjs": "catalog:",
    "dotenv": "catalog:",
    "express": "catalog:",
    "lodash": "catalog:",
    "multer": "catalog:",
    "nest-winston": "catalog:",
    "nestjs-i18n": "catalog:",
    "passport": "catalog:",
    "passport-custom": "catalog:",
    "passport-github2": "catalog:",
    "passport-jwt": "catalog:",
    "passport-local": "catalog:",
    "pg": "catalog:",
    "redis": "catalog:",
    "reflect-metadata": "catalog:",
    "rimraf": "catalog:",
    "rxjs": "catalog:",
    "sharp": "catalog:",
    "socket.io": "catalog:",
    "typeorm": "catalog:",
    "uuid": "catalog:",
    "winston": "catalog:"
  },
  "devDependencies": {
    "@nestjs/cli": "catalog:",
    "@nestjs/schematics": "catalog:",
    "@nestjs/testing": "catalog:",
    "@types/bcrypt": "catalog:",
    "@types/express": "catalog:",
    "@types/jest": "catalog:",
    "@types/lodash": "catalog:",
    "@types/multer": "catalog:",
    "@types/node": "catalog:",
    "@types/passport-github2": "catalog:",
    "@types/passport-jwt": "catalog:",
    "@types/passport-local": "catalog:",
    "@types/supertest": "catalog:",
    "@types/uuid": "catalog:",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "eslint": "catalog:",
    "jest": "catalog:",
    "source-map-support": "catalog:",
    "supertest": "catalog:",
    "ts-jest": "catalog:",
    "ts-loader": "catalog:",
    "ts-node": "catalog:",
    "tsconfig-paths": "catalog:",
    "typescript": "catalog:"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
